{
    "name": "soapbox/idempotency",
    "type": "library",
    "description": "A Laravel library that manages idempotency keys for a request",
    "license": "MIT",
    "require": {
        "ramsey/uuid": "^3.8",
        "guzzlehttp/psr7": "^1.4",
        "illuminate/support": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\Idempotency\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chocofamily\\Idempotency\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "orchestra/testbench": "^3.8",
        "jshayes/fake-requests": "^2.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chocofamily\\Idempotency\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "/vendor/bin/phpunit"
    }
}
