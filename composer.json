{
    "name": "chocofamilyme/idempotency",
    "type": "library",
    "description": "A Laravel library that manages idempotency keys for a request",
    "license": "MIT",
    "require": {
        "ramsey/uuid": "^3.8",
        "guzzlehttp/psr7": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Chocofamily\\Idempotency\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chocofamily\\Idempotency\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chocofamily\\Idempotency\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
